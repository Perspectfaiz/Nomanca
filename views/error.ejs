<%=layout("/layouts/boilerplate")%>
<div class="row mt-4 error-row">
    
    <div class="alert alert-danger col-6 offset-3" role="alert">
        <!-- <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Danger:"><use xlink:href="#exclamation-triangle-fill"/></svg> -->
        <h4 class="alert-heading"><%=  err.message %></h4>
        <hr>
        <p class="alert-para"><%= err.stack %></p>
    </div>
    <form action="/listings" method="GET" class="error-form">
        <button class="btn btn-dark">Explore Listings!</button>
    </form>
</div>
